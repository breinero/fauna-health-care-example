function AccessRecord (
  record: Record, 
  accessor: Researcher | Practitioner, 
  type: "Disclosure" | "Export", 
): Record | Null {

  let purpose = if (accessor isa Practitioner) {
    "Treatment"
  } else {
    "Research"
  }

  Event.create({
    record: record,
    accessor: accessor,
    type: type,
    purpose: purpose
  })

  record
}
